{
  "name": "apidragark",
  "version": "1.0.0",
  "description": "Api Dragark",
  "author": "Ftech",
  "license": "ISC",
  "scripts": {
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "build": "tsc && tsc -b",
    "dev": "tsc && tsc -b && nodemon -e ts --exec ts-node src/server.ts",
    "start": "tsc && tsc -b && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/server.js",
    "migrate:create": "typeorm migration:create -n",
    "migrate:run": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/migration.js up",
    "migrate:down": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/migration.js down",
    "migrate-tool:create": "cd ./src/migrationsTool && typeorm migration:create -n",
    "migrate-tool:run": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/migrationTool.js up",
    "migrate-tool:down": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/migrationTool.js down"
  },
  "dependencies": {
    "@koa/cors": "3.4.3",
    "@koa/multer": "^3.0.2",
    "@koa/router": "9.4.0",
    "@types/cors": "2.8.13",
    "@types/node": "18.11.18",
    "@types/nodemailer": "6.4.7",
    "axios": "^1.3.4",
    "bignumber.js": "^9.1.1",
    "bs58": "5.0.0",
    "bull": "4.10.3",
    "class-transformer": "0.4.0",
    "class-validator": "0.12.2",
    "crypto": "^1.0.1",
    "dotenv": "16.0.3",
    "fs": "^0.0.1-security",
    "handlebars": "4.7.7",
    "http": "^0.0.1-security",
    "http-status-codes": "2.2.0",
    "ip": "^1.1.8",
    "jsonwebtoken": "8.5.1",
    "kafkajs": "^2.2.4",
    "koa": "2.14.1",
    "koa-bodyparser": "4.3.0",
    "koa-socket": "^4.4.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.0.2",
    "node-cron": "^3.0.2",
    "pg": "8.3.0",
    "pg-connection-string": "^2.5.0",
    "redis": "^4.6.5",
    "reflect-metadata": "0.1.13",
    "signale": "^1.4.0",
    "socket.io": "4.6.1",
    "starknet": "6.7.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typedi": "0.8.0",
    "typeorm": "0.2.31",
    "typeorm-naming-strategies": "2.0.0",
    "typeorm-typedi-extensions": "0.2.3",
    "typescript": "4.9.5",
    "undici": "^5.22.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/bull": "4.10.0",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash": "^4.17.4",
    "@types/uuid": "3.4.10",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "concurrently": "7.6.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-prettier": "3.1.4",
    "nodemon": "2.0.20",
    "path": "0.12.7",
    "prettier": "2.8.3",
    "ts-node": "10.9.1",
    "tweetnacl-ts": "1.0.3",
    "typescript": "4.9.5"
  }
}
